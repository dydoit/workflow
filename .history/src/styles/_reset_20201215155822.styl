/*css reset*/
body, ul, li, ol, dl, dd, dt, p, h1, h2, h3, h4, h5, h6, form,img
  margin: 0;
  padding 0
button,
input,
optgroup,
select,
textarea
  font-family: inherit; /* 1 */
  font-size: 100%; /* 1 */
  line-height: 1.15; /* 1 */
  margin: 0; /* 2 */
i
  font-style: normal
ul,
ol
  list-style none
a 
  text-decoration: none
img
  border 0
  vertical-align middle /*  去掉图片低测默认的3像素空白缝隙*/
/*html5*/
article,aside,dialog,footer,header,section,footer,nav,figure,menu{display:block;}
// 局部滚动
html,body
  width 100%
  height 100%
  background-color: #f3f3f3
  overflow hidden